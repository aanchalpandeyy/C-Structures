#include<stdio.h>
struct employee
{
    int age,phone,salary;
    char name[25];
}emp[20];
void main()
{
    int i,n;
    for(i=0;i<n;i++)
    {
        scanf("%d %s %d %d",&emp[i].name,emp[i].age,&emp[i].phone,&emp[i].salary);
    }
    printf("\nNAME\tAGE\t\tPHONE NUMBER\tSALARY\n");
    for(i=0;i<n;i++)
    {
        printf("%s\t\t%d\t\t%d\t\t%d\n",emp[i].name,emp[i].age,emp[i].phone,emp[i].salary);
    }
}
